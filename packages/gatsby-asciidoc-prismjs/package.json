{
  "name": "gatsby-asciidoc-prismjs",
  "version": "1.0.0",
  "description": "gatsby-transformer-asciidoc extension to register prismjs as syntax highlighter",
  "author": "Lasse Knudsen",
  "license": "MIT",
  "main": "index.js",
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-asciidoc-prismjs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-asciidoc-prismjs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "asciidoctor": "^2.0.0",
    "asciidoctor-prism-extension": "^2.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.1.4",
    "lodash": "^4.17.10"
  },
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "asciidoc",
    "syntax-highlighter"
